{
  "name": "qurious",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "spellcheck": "cspell \"**\"",
    "test": "turbo run test",
    "prepare": "husky install"
  },
  "pnpm": {
    "overrides": {
      "markdown-it": "13.0.2"
    }
  },
  "devDependencies": {
    "convex": "^1.31.2",
    "cspell": "^9.6.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "turbo": "^2.5.3",
    "typescript": "5.8.2",
    "vercel": "^50.1.3"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=18"
  }
}